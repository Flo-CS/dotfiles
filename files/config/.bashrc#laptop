#!/usr/bin/env bash

source ~/.local/share/myarchy/files/config/bash/myarchy-env.sh
source ~/.local/share/myarchy/files/config/bash/env.sh
source ~/.local/share/myarchy/files/config/bash/functions.sh
source ~/.local/share/myarchy/files/config/bash/aliases.sh

if command -v starship &>/dev/null; then
	eval "$(starship init bash)"
fi

if command -v zoxide &>/dev/null; then
	eval "$(zoxide init bash)"
fi

. "$HOME/.atuin/bin/env"
[[ -f ~/.bash-preexec.sh ]] && source ~/.bash-preexec.sh
eval "$(atuin init bash)"

. "$HOME/.cargo/env"

. /usr/share/nvm/init-nvm.sh
